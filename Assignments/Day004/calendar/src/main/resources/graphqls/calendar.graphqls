schema {
    query: Query
    mutation: Mutation
}

type Calendar {
    holidayId:ID!
    event:String
    category:String
    holidayDate:String
    country:Country
}

type Country{
    countryCode:String
}

input CalendarInput{
    event:String
    category:String
    holidayDate:String
    countryCode: String
}

type Query {
    findAllCalendars:[Calendar]
    findCalendersByCountry(countryId:String):Calendar
}

Type Mutation{
    createCalendar(calendarInput:CalenderInput):Calendar
}